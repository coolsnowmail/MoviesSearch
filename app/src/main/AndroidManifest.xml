<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.amsdevelops.filmssearch">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.example.moviessearch.App"
        android:allowBackup="true"
        android:icon="@drawable/baseline_insert_emoticon_24"
        android:label="@string/app_name"
        android:roundIcon="@drawable/baseline_insert_emoticon_24"
        android:supportsRtl="true"
        android:theme="@style/Theme.MoviesSearch">
        <activity
            android:name="com.example.moviessearch.view.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>